<div class="jumbotron text-center">
    <h1>All Balances</h1>
</div>

<table class="table table-bordered table-hover table-striped">
    <thead>
        <tr>
            <th>Wallet Address</th>
            <th>Balance</th>
        </tr>
    </thead>
    <tbody>
        
        <% for (var bal of bals) { %>
            <tr>
                <td><%= bal.address %></td>
                <td><%= bal.balance %></td>
            </tr>
        <% } %>

    </tbody>
</table>